  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "body-parser": "1.15.2",
    "common-tags": "1.4.0",
    "ect": "0.5.9",
