{
  "name": "Saider",
  "version": "0.6.0",
  "private": true,
  "description": "A interactive dice bot",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node .",
    "dev": "cross-env NODE_ENV=develop babel-node server/server.js --presets es2015-node6 --plugins add-module-exports",
    "test": "npm run lint && npm run build",
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel server -d lib",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.config.js --progress --hide-modules",
    "lint": "eslint -c config/eslintrc.js --ext .js,.vue client server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NKMR6194/saider.git"
  },
  "author": "SAKATA Sinji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NKMR6194/saider/issues"
  },
  "homepage": "https://github.com/NKMR6194/saider",
  "engines": {
    "node": "^6.9.1",
    "npm": "^5.0.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "bcdice-js": "^0.6.1",
    "body-parser": "1.15.2",
    "common-tags": "1.4.0",
    "express": "4.14.0",
    "helmet": "3.1.0",
    "mdi": "^2.0.46",
    "mongodb": "^2.2.31",
    "nanoid": "^0.2.1",
    "socket.io": "1.5.1",
    "vue": "^2.3.3",
    "vue-material": "^0.7.4",
    "vue-socket.io": "^2.1.1-a",
    "vue-toast": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.9.0",
    "jsonp": "^0.2.1",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.18.2",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.4.5"
  }
}
